{
  "private": true,
  "name": "@livekit/components-docs-gen",
  "version": "0.0.20",
  "description": "Generate component docs.",
  "license": "Apache 2.0",
  "author": "LiveKit",
  "repository": {
    "type": "git",
    "url": "https://github.com/livekit/components-js.git",
    "directory": "eslint-plugin-tsdoc"
  },
  "scripts": {
    "gen:docs": "yarn gen:api-extractor && yarn gen:api-documenter",
    "gen:api-extractor": "tsx src/gen-docs.ts",
    "gen:api-documenter": "api-documenter markdown --input-folder ../../packages/react/temp --output-folder ../../docs/beta-docs",
    "lint": "eslint -f unix \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "dependencies": {
    "@livekit/api-documenter": "~7.22.17",
    "@livekit/components-react": "~1.0.0",
    "eslint-config-lk-custom": "*"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.22.16",
    "@microsoft/api-extractor": "^7.35.2"
  },
  "keywords": [
    "docs",
    "props"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}
